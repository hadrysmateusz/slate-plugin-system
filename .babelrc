{
  "presets": [
    [
      "@babel/preset-env",
      {
        "modules": false
      }
    ],
    "@babel/preset-react",
    "@babel/preset-typescript"
  ],
  "plugins": [
    "@babel/plugin-proposal-optional-chaining",
    "@babel/plugin-proposal-nullish-coalescing-operator"
  ],
  "extensions": [".js", ".jsx", ".es6", ".es", ".mjs", ".ts", ".tsx"],
  "exclude": "node_modules/**"
}
